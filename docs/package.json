{
  "name": "docs",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run build:bundle && npm run build:svelte",
    "build:bundle": "cd .. && npm i && npm run bundle",
    "build:svelte": "vite build",
