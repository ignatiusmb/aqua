{
  "name": "@ignatiusmb/aqua",
  "author": "Ignatius Bagus",
  "homepage": "https://aqua.mauss.dev",
  "description": "Native DOM enhancements",
  "version": "21.0.0",
  "license": "MIT",
  "type": "module",
  "types": "./index.d.ts",
  "scripts": {
    "docs": "npm run docs:js && npm run docs:css",
    "docs:js": "rollup -c -w",
    "docs:css": "postcss src/aqua.css -o lib/aqua.min.css -w",
    "check": "pnpm check:style",
    "check:style": "prettier -c src",
    "pub:build": "tsc",
    "pub:format": "prettier -w .",
    "prepublishOnly": "pnpm pub:build && pnpm pub:format"
  },
  "exports": {
    "./package.json": "./package.json"
  },
  "packageManager": "pnpm@7.17.1",
  "prettier": "mauss/prettier.json",
  "repository": "github:ignatiusmb/aqua",
  "bugs": "https://github.com/ignatiusmb/aqua/issues",
  "keywords": [
    "aqua",
    "DOM",
    "enhancements",
    "UI",
    "pure",
    "JS",
    "CSS"
  ],
  "devDependencies": {
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/prismjs": "^1.26.0",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.13",
    "mauss": "^0.4.6",
    "postcss-cli": "^10.0.0",
    "postcss-header": "^3.0.2",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.2.1",
    "prismjs": "^1.23.0",
    "rollup": "^2.36.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.9.4"
  }
}
