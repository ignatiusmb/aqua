{
  "name": "@ignatiusmb/aqua",
  "author": "Ignatius Bagus",
  "homepage": "https://aqua.mauss.dev",
  "description": "Code Syntax Highlighter",
  "version": "21.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "watch": "pnpm pub:build --watch",
    "check": "pnpm check:style",
    "check:style": "prettier -c src",
    "pub:build": "tsc --declaration",
    "pub:format": "prettier -w .",
    "prepublishOnly": "pnpm pub:build && pnpm pub:format"
  },
  "exports": {
    ".": "./index.js",
    "./marker": "./marker.js",
    "./styles/var.css": "./styles/var.css",
    "./styles/code.css": "./styles/code.css",
    "./package.json": "./package.json"
  },
  "packageManager": "pnpm@7.17.1",
  "prettier": "mauss/prettier.json",
  "repository": "github:ignatiusmb/aqua",
  "bugs": "https://github.com/ignatiusmb/aqua/issues",
  "keywords": [
    "preprocessor",
    "markdown",
    "syntax",
    "highlighter"
  ],
  "dependencies": {
    "shiki": "^0.11.1"
  },
  "devDependencies": {
    "mauss": "^0.4.6",
    "prettier": "^2.2.1",
    "typescript": "^4.9.4"
  }
}
